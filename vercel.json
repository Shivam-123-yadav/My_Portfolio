{
  "version": 2,
  "builds": [
    {
      "src": "Base/manage.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb", "runtime": "Python3.13.2" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Base/manage.py"
    }
  ]
}